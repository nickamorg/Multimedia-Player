<grammar version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="rootRule">
    <one-of>
      <item>
        <tag>FORWARD</tag>
        <one-of>
          <item> forwards </item>
          <item> forward </item>
          <item> straight </item>
        </one-of>
      </item>
	  
      <item>
        <tag>BACKWARD</tag>
        <one-of>
          <item> backward </item>
          <item> backwards </item>
          <item> back </item>
        </one-of>
      </item>
	  
       <item>
        <tag>PLAY</tag>
        <one-of>
			<item> play </item>
			<item> start </item>
			<item> begin </item>
			<item> continue </item>
			<item> play the song </item>
			<item> play </item>
			<item> play music </item>
			<item> play the movie </item>
			<item> play the episode</item>
        </one-of>
      </item>
	  
       <item>
        <tag>PAUSE</tag>
        <one-of>
          <item> pause </item>
		  <item> stop </item>
		  <item> standby </item>
          <item> pause the song </item>
          <item> pause now </item>
             <item> pause music </item>
              <item>pause the movie </item>
           <item> pause the episode</item>
        </one-of>
      </item>
	  
      <item>
        <tag>LEFT</tag>
        <one-of>
          <item> turn left </item>
          <item> swipe left</item>
          <item> next </item>
        </one-of>
      </item>
	  
      <item>
        <tag>RIGHT</tag>
        <one-of>
          <item> turn right </item>
          <item> swipe right </item>
          <item> previus </item>
		  <item> prev </item>
        </one-of>
      </item>
	  
       <item>
        <tag>UP</tag>
        <one-of>
          <item> up </item>
          <item> swipe up </item>
          <item> volume up </item>
		  <item> increase </item>
        </one-of>
      </item>
	  
       <item>
        <tag>DOWN</tag>
        <one-of>
          <item> swipe down </item>
          <item> down </item>
          <item> volume down </item>
		  <item> decrease </item>
        </one-of>
      </item>
	  
	  <item>
        <tag>LOBBY</tag>
        <one-of>
          <item> lobby </item>
          <item> three player </item>
          <item> triple player </item>
        </one-of>
      </item>
	  
	  <item>
        <tag>EXPLORE</tag>
        <one-of>
          <item> Explore </item>
          <item> Home </item>
          <item> Songs </item>
		  <item> movies </item>
		  <item> series </item>
        </one-of>
      </item>
	  
	  <item>
        <tag>Playlists</tag>
        <one-of>
          <item> Playlists </item>
          <item> Playlist </item>
          <item> list </item>
        </one-of>
      </item>
	  
	  <item>
        <tag>My</tag>
        <one-of>
          <item> my </item>
          <item> my songs </item>
          <item> my movies </item>
		  <item> my series </item>
        </one-of>
      </item>
	  
	  <item>
        <tag>SEARCH</tag>
        <one-of>
          <item> search </item>
          <item> find </item>
          <item> look for </item>
		  <item> seek </item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>
